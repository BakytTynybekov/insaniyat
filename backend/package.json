{
  "name": "@insaniyat/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --watch ./.env --rs --exit-child ./src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "start": "node ./dist/index.js",
    "prisma": "prisma",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma",
    "pmd": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "pgc": "prisma generate --schema=./src/prisma/schema.prisma",
    "pmr": "prisma migrate reset --schema=./src/prisma/schema.prisma",
    "types": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.824.0",
    "@aws-sdk/s3-request-presigner": "^3.824.0",
    "@insaniyat/shared": "workspace:*",
    "@insaniyat/webapp": "workspace:*",
    "@prisma/client": "^6.11.1",
    "@trpc/server": "^10.45.2",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "randomstring": "^1.3.1",
    "superjson": "^2.2.2",
    "trpc-playground": "^1.0.4",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.4",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/randomstring": "^1.3.0",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.5.1",
    "prisma": "^6.11.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
