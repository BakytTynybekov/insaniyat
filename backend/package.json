{
  "name": "@insaniyat/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --rs --exit-child ./src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "start": "node ./dist/index.js",
    "prisma": "prisma",
    "pmd": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "pgc": "prisma generate --schema=./src/prisma/schema.prisma",
    "pmr": "prisma migrate reset --schema=./src/prisma/schema.prisma",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "prepare": "pnpm pgc"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@trpc/server": "^10.45.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "superjson": "^2.2.2",
    "trpc-playground": "^1.0.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.4",
    "prettier": "^3.5.1",
    "prisma": "^6.5.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
